double myPow(double x, int n) {
    long long num = n;
    double result = 1.0;
    if (num < 0)
    {
        x = 1 / x;
        num = -num;
    }
    while (num)
     {
        if (num % 2)
         {
            result *= x;
         }
        x *= x;
        num /= 2;
    }
    return result;
}
